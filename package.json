  "private": true,
  "scripts": {
    "dev": "nodemon --exec \"npx tsx server.ts\" --watch server.ts --watch src --ext ts,tsx,js,jsx",
    "build": "next build",
    "start": "NODE_ENV=production tsx server.ts",
    "lint": "next lint",
    "test": "rimraf ./prisma/test.db && cross-env DATABASE_URL=file:./prisma/test.db npx prisma db push && vitest",
    "test:watch": "vitest --watch",
    "test:aws": "tsx scripts/test-aws-new.ts",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:migrate:aws": "prisma migrate deploy --schema=prisma/schema.postgresql.prisma",
    "db:migrate:aws:prod": "powershell -Command \"Get-Content .env.production | ForEach-Object { if($_ -match '^([^=]+)=(.*)') { [Environment]::SetEnvironmentVariable($matches[1], $matches[2], 'Process') } }; npx prisma migrate deploy --schema=prisma/schema.postgresql.prisma\"",
    "db:push:aws": "prisma db push --schema=prisma/schema.postgresql.prisma",
    "db:generate:aws": "prisma generate --schema=prisma/schema.postgresql.prisma",
    "aws:deploy": "powershell -ExecutionPolicy Bypass -File scripts/aws-deploy.ps1",
    "aws:deploy:linux": "bash scripts/aws-deploy.sh",
    "aws:status": "powershell -ExecutionPolicy Bypass -File scripts/aws-status.ps1",
    "aws:status:linux": "bash scripts/aws-status.sh",
    "aws:setup": "bash scripts/setup-aws.sh",
    "aws:health": "curl http://localhost:3000/api/health/aws"
  },
